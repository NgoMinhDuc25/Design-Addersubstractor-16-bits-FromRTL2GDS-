source ./scripts/synthesis.tcl
read_hdl {./rtl/addersubtractor_top.v ./rtl/addersubtractor.v ./rtl/mux2to1.v ./rtl/adderk.v}
elaborate $DESIGN
set_dont_touch [get_cells -hier "*pad*"]
