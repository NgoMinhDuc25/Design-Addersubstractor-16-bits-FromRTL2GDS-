syn_generic
time_info GENERIC
set_attribute syn_map_effort $MAP_OPT_EFF
syn_map
time_info MAPPED
set_attribute syn_opt_effort $MAP_OPT_EFF
syn_opt
time_info OPT
write_snapshot -outdir $_REPORTS_PATH -tag final
report_summary -outdir $_REPORTS_PATH
